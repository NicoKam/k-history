{
  "name": "k-history",
  "version": "0.0.1",
  "main": "./lib/index.js",
  "files": [
    "lib/"
  ],
  "scripts": {
    "build": "npm run clear && tsc -p config/tsconfig.prod.json",
    "clear": "rimraf lib dist esm",
    "prepublishOnly": "npm run build",
    "eslint": "eslint --fix --ext .js --ext .jsx --ext .ts --ext .tsx ./src",
    "pub": "npm publish --registry=https://registry.npmjs.org/ && git push",
    "watch": "tsc -p ./tsconfig.json --watch"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "events": "^3.2.0",
    "qs": "^6.9.4",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@ali-whale/eslint-config": "^0.9.0-beta.9",
    "@types/chokidar": "^2.1.3",
    "@types/cross-spawn": "^6.0.2",
    "@types/events": "^3.0.0",
    "@types/jest": "^24.0.12",
    "@types/node": "^12.19.11",
    "@types/qs": "^6.9.5",
    "eslint": "^7.17.0",
    "husky": "^4.3.7",
    "jest": "^24.9.0",
    "lint-staged": "^10.5.3",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "typescript": "^4.1.3"
  },
  "author": "NicoKam",
